{
  "name": "jonston/amqp-laravel",
  "description": "Simple wrapper for amqp library to use in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene",
      "email": "jonston85@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "php-amqplib/php-amqplib": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jonston\\AmqpLaravel\\AMQPServiceProvider"
      ],
      "commands": [
        "Jonston\\AmqpLaravel\\Console\\Commands\\AMQPSetupCommand"
      ]
    }
  }
}
