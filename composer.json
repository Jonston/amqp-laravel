{
  "version": "0.1.0",
  "name": "jonston/amqp-laravel",
  "description": "Simple wrapper for amqp library to use in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene",
      "email": "jonston@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "php-amqplib/php-amqplib": "^3.0",
    "illuminate/config": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^9.0|^10.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\Tests\\": "tests/"
    }
  }
}