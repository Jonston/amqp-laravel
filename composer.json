{
  "name": "jonston/amqp-laravel",
  "description": "Simple wrapper for amqp library to use in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene",
      "email": "jonston85@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "php-amqplib/php-amqplib": "^3.0",
    "laravel/framework": ">=9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jonston\\AmqpLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jonston\\AmqpLaravel\\AMQPServiceProvider"
      ]
    }
  }
}